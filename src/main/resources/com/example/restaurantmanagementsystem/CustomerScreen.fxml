<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.restaurantmanagementsystem.CustomerScreenCtrl">
   <children>
      <Button fx:id="backBtn" layoutX="22.0" layoutY="350.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="56.0" text="Back" />
      <Button fx:id="menuBtn" layoutX="131.0" layoutY="266.0" mnemonicParsing="false" onAction="#navigateMenuPage" prefHeight="67.0" prefWidth="149.0" text="Our Prices" />
      <Button fx:id="galleryBtn" layoutX="320.0" layoutY="266.0" mnemonicParsing="false" onAction="#navigateGalleryPage" prefHeight="67.0" prefWidth="149.0" text="Check Our Gallery!" />
      <TableView fx:id="mealsStackTable" layoutX="320.0" layoutY="31.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="foodNameColumn" prefWidth="102.0" text="Meal " />
            <TableColumn fx:id="foodPriceColumn" prefWidth="102.0" text="Price" />
        </columns>
      </TableView>
      <TableView fx:id="orderQueueTable" layoutX="94.0" layoutY="31.0" prefHeight="200.0" prefWidth="200.0">
         <columns>
            <TableColumn prefWidth="199.0" text="Order Queue" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
